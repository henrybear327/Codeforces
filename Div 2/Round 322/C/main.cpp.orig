#include <bits/stdc++.h>
// LLONG_MIN LLONG_MAX INT_MIN INT_MAX

#ifdef _WIN32
#define lld "I64d"
#else
#define lld "lld"
#endif

using namespace std;

bool cmp(pair<int, int> a, pair<int, int> b) { return a.first < b.first; }

int main() {
  int tot, unit;
  scanf("%d %d", &tot, &unit);

  pair<int, int> inp[100010];
  for (int i = 0; i < tot; i++) {
    scanf("%d", &inp[i].second);
    inp[i].second = 100 - inp[i].second;
    inp[i].first = inp[i].second % 10;
  }

  sort(inp, inp + tot, cmp);

  for (int i = 0; i < tot && unit; i++) {
    if (unit >= inp[i].first) {
      unit -= inp[i].first;
      inp[i].first = inp[i].second - inp[i].second % 10;
    } else {
      inp[i].first = inp[i].second - unit;
      unit = 0;
    }
    printf("%d\n", inp[i].first);
  }

  sort(inp, inp + tot, cmp);

  for (int i = 0; i < tot && unit; i++) {
    if (unit >= inp[i].first) {
      unit -= inp[i].first;
      inp[i].first = 0;
    } else {
      inp[i].first -= unit;
      unit = 0;
    }
  }

  int ans = 0;
  for (int i = 0; i < tot; i++) {
    printf("%d\n", 100 - inp[i].first);
    ans += (100 - inp[i].first) / 10;
  }
  printf("%d\n", ans);

  return 0;
}
