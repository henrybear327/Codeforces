#include <bits/stdc++.h>
//LLONG_MIN LLONG_MAX INT_MIN INT_MAX

#ifdef _WIN32
#define lld "I64d"
#else
#define lld "lld"
#endif

using namespace std;

long long int gcd(long long int a, long long int b)
{
    return b == 0 ? a : gcd(b, a % b);
}


int main()
{
    int t, w, b;
    scanf("%d %d %d", &t, &w, &b);
    return 0;
}
